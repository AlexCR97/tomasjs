{
  "name": "@tomasjs/express",
  "version": "1.4.0",
  "description": "Support for express in a TomasJS infrastructure.",
  "author": "viex",
  "license": "ISC",
  "types": "./**/*.d.ts",
  "keywords": [
    "tomas",
    "tomasjs",
    "typescript",
    "viex",
    "express"
  ],
  "scripts": {
    "compile-clean": "del dist /s /q",
    "compile-build": "tsc -b && tsc-alias",
    "compile": "npm run compile-clean && npm run compile-build && node ./dist/SetupPackage.js",
    "publish": "cd ./dist && npm publish --access public",
    "pack": "cd ./dist && npm pack",
    "link": "cd ./dist && npm link",
    "test": "jest",
    "testw": "jest --watchAll --verbose",
    "testw-ExpressAppBuilder": "jest --testNamePattern ExpressAppBuilder --watchAll --verbose",
    "testw-auth-jwt-guard": "jest --testNamePattern auth-jwt-guard --watchAll --verbose",
    "testw-auth-required-claim-guard": "jest --testNamePattern auth-required-claim-guard --watchAll --verbose",
    "testw-builder": "jest --testNamePattern builder --watchAll --verbose",
    "testw-configuration": "jest --testNamePattern configuration --watchAll --verbose",
    "testw-controllers": "jest --testNamePattern controllers --watchAll --verbose",
    "testw-cqrs-commands": "jest --testNamePattern cqrs-commands --watchAll --verbose",
    "testw-cqrs-events": "jest --testNamePattern cqrs-events --watchAll --verbose",
    "testw-cqrs-queries": "jest --testNamePattern cqrs-queries --watchAll --verbose",
    "testw-endpoints": "jest --testNamePattern endpoints --watchAll --verbose",
    "testw-endpoint-groups": "jest --testNamePattern endpoint-groups --watchAll --verbose",
    "testw-endpoint-isEndpoint": "jest --testNamePattern isEndpoint --watchAll --verbose",
    "testw-endpoint-metadata": "jest --testNamePattern EndpointMetadata --watchAll --verbose",
    "testw-endpoint-decorator": "jest --testNamePattern endpoint-decorator --watchAll --verbose",
    "testw-endpoint-path-decorator": "jest --testNamePattern path-decorator --watchAll --verbose",
    "testw-error-handler": "jest --testNamePattern error-handler --watchAll --verbose",
    "testw-fluentvalidation-middleware": "jest --testNamePattern fluentvalidation-middleware --watchAll --verbose",
    "testw-guards": "jest --testNamePattern guards --watchAll --verbose",
    "testw-logger": "jest --testNamePattern logger --watchAll --verbose",
    "testw-sample-crud": "jest --testNamePattern crud-sample --watchAll --verbose",
    "testw-pipes-bodyPipe-decorator": "jest --testNamePattern bodyPipe-decorator --watchAll --verbose --detectOpenHandles",
    "testw-pipes-paramPipe-decorator": "jest --testNamePattern paramPipe-decorator --watchAll --verbose --detectOpenHandles",
    "testw-pipes-queryPipe-decorator": "jest --testNamePattern queryPipe-decorator --watchAll --verbose --detectOpenHandles",
    "testw-transforms-number-transform": "jest --testNamePattern number-transform --watchAll --verbose --detectOpenHandles",
    "testw-transforms-instance-transform": "jest --testNamePattern instance-transform --watchAll --verbose --detectOpenHandles",
    "watch-examples": "ts-node -r tsconfig-paths/register ./src/examples/examples.ts",
    "serve-sample-controllers": "ts-node -r tsconfig-paths/register ./samples/crud-with-controllers/main.ts",
    "watch-sample-controllers": "nodemon -r tsconfig-paths/register ./samples/crud-with-controllers/main.ts",
    "serve-sample-endpoints": "ts-node -r tsconfig-paths/register ./samples/crud-with-endpoints/main.ts",
    "watch-sample-endpoints": "nodemon -r tsconfig-paths/register ./samples/crud-with-endpoints/main.ts",
    "serve-sample-jwt-authentication": "ts-node -r tsconfig-paths/register ./samples/jwt-authentication/main.ts",
    "watch-sample-jwt-authentication": "nodemon -r tsconfig-paths/register ./samples/jwt-authentication/main.ts"
  },
  "dependencies": {
    "@tomasjs/core": "^1.3.1",
    "@tomasjs/logging": "^1.4.2",
    "class-transformer": "^0.5.1",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-async-errors": "^3.1.1",
    "fluentvalidation-ts": "^2.3.0",
    "jsonwebtoken": "^9.0.0"
  },
  "peerDependencies": {
    "@tomasjs/core": "^1.3.1",
    "@tomasjs/logging": "^1.4.2",
    "class-transformer": "^0.5.1",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-async-errors": "^3.1.1",
    "fluentvalidation-ts": "^2.3.0",
    "jsonwebtoken": "^9.0.0"
  },
  "devDependencies": {
    "@jest/globals": "^29.3.1",
    "@tomasjs/cqrs": "^1.0.0",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.14",
    "@types/jest": "^29.2.3",
    "@types/jsonwebtoken": "^8.5.9",
    "@types/node-fetch": "^2.6.2",
    "jest": "^29.3.1",
    "node-fetch": "^2.6.7",
    "nodemon": "^2.0.20",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.9.1",
    "tsc-alias": "^1.8.2",
    "tsconfig-paths": "^4.1.0",
    "typescript": "^4.9.4"
  }
}