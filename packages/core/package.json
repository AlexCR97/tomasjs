{
  "name": "@tomasjs/core",
  "version": "1.0.1",
  "description": "The core components of TomasJS.",
  "author": "viex",
  "license": "ISC",
  "types": "./**/*.d.ts",
  "keywords": [
    "tomas",
    "tomasjs",
    "typescript",
    "viex",
    "core"
  ],
  "scripts": {
    "compile-clean": "del dist /s /q",
    "compile-build": "tsc -b && tsc-alias",
    "compile": "npm run compile-clean && npm run compile-build && node ./dist/SetupPackage.js",
    "publish": "cd ./dist && npm publish --access public",
    "sample": "nodemon -r tsconfig-paths/register ./sample/main.ts"
  },
  "dependencies": {
    "inversify": "^6.0.1",
    "winston": "^3.8.2"
  },
  "peerDependencies": {
    "inversify": "^6.0.1",
    "winston": "^3.8.2"
  },
  "devDependencies": {
    "nodemon": "^2.0.22",
    "reflect-metadata": "^0.1.13",
    "ts-node": "^10.9.1",
    "tsc-alias": "^1.8.5",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^4.9.4"
  }
}